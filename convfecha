#!/bin/bash 

sed -e '{s/(\([0-9]\{4\}\)01\([0-9]\{2\}\))/[\2-ENE-\1]/g};
        {s/(\([0-9]\{4\}\)02\([0-9]\{2\}\))/[\2-FEB-\1]/g};
        {s/(\([0-9]\{4\}\)03\([0-9]\{2\}\))/[\2-MAR-\1]/g};
        {s/(\([0-9]\{4\}\)04\([0-9]\{2\}\))/[\2-ABR-\1]/g};
        {s/(\([0-9]\{4\}\)05\([0-9]\{2\}\))/[\2-MAY-\1]/g};
        {s/(\([0-9]\{4\}\)06\([0-9]\{2\}\))/[\2-JUN-\1]/g};
        {s/(\([0-9]\{4\}\)07\([0-9]\{2\}\))/[\2-JUL-\1]/g};
        {s/(\([0-9]\{4\}\)08\([0-9]\{2\}\))/[\2-AGO-\1]/g};
        {s/(\([0-9]\{4\}\)09\([0-9]\{2\}\))/[\2-SEP-\1]/g};
        {s/(\([0-9]\{4\}\)10\([0-9]\{2\}\))/[\2-OCT-\1]/g};
        {s/(\([0-9]\{4\}\)11\([0-9]\{2\}\))/[\2-NOV-\1]/g};
        {s/(\([0-9]\{4\}\)12\([0-9]\{2\}\))/[\2-DIC-\1]/g}; 
        '